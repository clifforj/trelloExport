{
	"name": "TrelloExport",
	"version": "1.2.2",
	"manifest_version" : 2,
	
	"description": "Export Trello Boards to Excel - With Comments & Checklists (JC)",
	"icons": { "128": "icons/icon128.png" },
	
	"content_scripts": [
		{
			"matches": ["https://trello.com/*"],
			"js": ["lib/jquery-1.8.3.min.js",
                   "lib/FileSaver.min.js",
                   "lib/xlsx.js",
                   "lib/jszip.js",
                   "lib/jszip-load.js",
                   "lib/jszip-inflate.js",
                   "lib/jszip-deflate.js",
                   "trelloexport.js"]
		}
	]
	
}


